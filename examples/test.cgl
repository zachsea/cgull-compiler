struct Weird {
  private float z = 5.0;

  fn test(int z) -> float {
    return (z as float) + this.z;
  }

  fn setZ(float z) {
    this.z = z;
  }

  fn $toString() -> string {
    return "Weird(z: " + z + ")";
  }
}

struct Point {
  int x;
  int y;
  Weird w;

  fn $toString() -> string {
    return "Point(x: " + x + ", y: " + y + ", w: " + (w as string) + ")";
  }
}

fn main() {
  Point p = Point(1, 2, Weird());
  p.w.setZ(4.0);
  println(p.w.test(10));
  println(p);
  return;
}
